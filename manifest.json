{
  "manifest_version": 2,
  "name": "Call-Tracking Assistant",
  "description": "Assists in Call Tracking for Rain Retail Tech Support",
  "version": "1",
  "author": "Jake Prins",
  "browser_action": {
    "default_popup": "newtab.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens newtab.html"
    }
  },
  "content_scripts": [
    {
      "js": ["background.js"],
      "matches": [
        "https://docs.google.com/forms/d/e/1FAIpQLScFVg5sYWwkVbCZHx1nCY6hKhkgMSpU2h9KjV_HdGHY-0OGVQ/viewform"
      ]
    }
  ],
  "permissions": ["activeTab", "webNavigation"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
